assign x = 5;
assign y = 10;
assign z = x + y;

if z > 10 {
    assign z = z + 1;
} else {
    if z < 10 {
        assign z = z + 1;
    } else {
        assign z = z + 10;
    }
    assign z = z - 2;
}

for assign i = 0; i < 10; assign i = i + 1; {
    assign z = z + (i * 2);
}

while z > 0 {
    assign z = z - 1;
}

return z; @ z = 0
