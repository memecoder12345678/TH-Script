assign x = 5;
assign y = 10;
assign z = 0;

if x + y > 10 {
    assign z = x * y;
    if z > 50 {
        assign z = z - 5;
    } else {
        assign z = z + 5;
    }
} else {
    assign z = x + y;
}

assign sum = 0;
for assign i = 0; i < 10; assign i = i + 1; {
    sum = sum + i;
}

assign sum = sum + z;

while sum > 0 {
    assign sum = sum - 1;
}

return sum;
